<domain name="Automobile">
  <search>
	<source name="HerzogMeierAutoCenter" url="http://www.herzogmeier.com/en_US/f_CombinedVehicleSearch_1.chtml"></source>
	<element>
	  <element vname="SEARCH" text = "search">
		<checkbox>
		  <item>New</item>
		  <item>Pre-Owned</item>
		</checkbox>
	  </element>
	  <element vname="MAKE" text = "Make">
		<selectbox>
		  <item>Any</item>
		  <item>Acura</item>
		  <item>Audi</item>
		  <item>BMW</item>
		  <item>Chevrolet</item>
		  <item>Chrysler</item>
		  <item>Dodge</item>
		  <item>Honda</item>
		  <item>Jaguar</item>
		  <item>Jeep</item>
		</selectbox>
	  </element>
	  <element vname="model" text ="MODEL/TYPE" depends="MAKE">
		<selectbox>
		  <item>Any</item>
		  <item>528i</item>
		  <item>740</item>
		  <item>850</item>
		  <item>A6</item>
		  <item>Accord</item>
		  <item>Alero</item>
		  <item>Avalon</item>
		  <item>C70</item>
		  <item>Cavalier</item>
		</selectbox>
	  </element>
	  <element vname="minYear" text ="YEAR RANGE From">
		  <selectbox>
			<item>Any</item>
			<item>Pre 1993</item>
			<item>1993</item>
			<item>1994</item>
			<item>1995</item>
			<item>1996</item>
			<item>1997</item>
			<item>1998</item>
			<item>1999</item>
			<item>2000</item>
		  </selectbox>
		</element>
		<element vname="maxYear" text ="To">
		  <selectbox>
			<item>Any</item>
			<item>Pre 1993</item>
			<item>1993</item>
			<item>1994</item>
			<item>1995</item>
			<item>1996</item>
			<item>1997</item>
			<item>1998</item>
			<item>1999</item>
			<item>2000</item>
		  </selectbox>
		</element>
	  <element vname="miles" text ="Miles">
		<selectbox>
		  <item>Any</item>
		  <item>Under 10,000 miles</item>
		  <item>Under 20,000 miles</item>
		  <item>Under 30,000 miles</item>
		  <item>Under 40,000 miles</item>
		  <item>Under 50,000 miles</item>
		  <item>Under 60,000 miles</item>
		  <item>Under 70,000 miles</item>
		  <item>Under 80,000 miles</item>
		  <item>Under 90,000 miles</item>
		</selectbox>
	  </element>
		<element vname="minPrice" text="From">
		  <selectbox>
			<item>Any</item>
			<item>$10,000.00</item>
			<item>$15,000.00</item>
			<item>$20,000.00</item>
			<item>$25,000.00</item>
			<item>$30,000.00</item>
			<item>$35,000.00</item>
			<item>$40,000.00</item>
			<item>$45,000.00</item>
			<item>$50,000.00</item>
		  </selectbox>
		</element>
		<element vname="maxPrice" text="To">
		  <selectbox>
			<item>Any</item>
			<item>$10,000.00</item>
			<item>$15,000.00</item>
			<item>$20,000.00</item>
			<item>$25,000.00</item>
			<item>$30,000.00</item>
			<item>$35,000.00</item>
			<item>$40,000.00</item>
			<item>$45,000.00</item>
			<item>$50,000.00</item>
		  </selectbox>
		</element>
	  <element vname ="resultsPerPage" text="results per page">
		<selectbox>
		  <item>10</item>
		  <item>25</item>
		  <item>50</item>
		  <item>100</item>
		</selectbox>
	  </element>
	  <element vname="showImages" >
		<checkbox>
		<item> Show thumbnail images in results </item>
		</checkbox>
	  </element>
	</element>
  </search>
</domain>

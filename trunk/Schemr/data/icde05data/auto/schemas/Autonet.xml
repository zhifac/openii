<domain name="Automobile">
	<search>          
		<source name="Autonet" url="http://autonetmail.com/UsedVehiclePriceQuote.asp?"></source>
	<element>
	  <element vname="email" text = "E-mail Address (Required)">
		<textbox></textbox>
	  </element>
	  <element vname="Telephone" text = "Telephone (recommended)include Area Code">
		<textbox></textbox>
	  </element>
	  <element vname="Name" text = "Name (required)">
		<textbox></textbox>
	  </element>
	  <element vname="Address" text ="Address">
		<textbox></textbox>
	  </element>
	  <element vname="City-State-Zip" text = "City-State-Zip">
		<textbox></textbox>
	  </element>
	  <element vname="Contact" text= "Mode of Contact">
		<RadioButton>
          <item>EMail</item>
          <item>Telephone</item>
        </RadioButton>
	  </element>
	  <element vname="Purchase" text = "Approximate Purchase Date">
		<checkbox>
		  <item>Now</item>
		  <item>1-2 Weeks</item>		  
		  <item>2-3 Weeks</item>		  
		  <item>1 Mo. or more</item>
		</checkbox>
	  </element>
	  <element vname="VehicleType" text ="Vehicle type">
		<checkbox>
		  <item>Sport/Ute</item>
		  <item>Car</item>
		  <item>Van</item>
		  <item>Truck</item>
		</checkbox>
	  </element>
	  <element vname="Make" text ="Make">
		<selectbox>
		  <item>Any</item>
		  <item>Acura</item>
		  <item>Alfa Romeo</item>
		</selectbox>
	  </element>
	  <element vname="Model" text = "Model(s)">
		<textbox></textbox>
	  </element>
	  <element vname="SecondMake" text ="2nd choice Make">
		<selectbox>
		  <item>none</item>
		  <item>Acura</item>
		  <item>Alfa Romeo</item>
		</selectbox>
	  </element>
	  <element vname="SecondModel" text = "2nd choice Model(s)">
		<textbox></textbox>
	  </element>
	  <element vname="Year" text ="Made In Or After">
		<selectbox>
		  <item>Any Year</item>
		  <item>1980-85</item>
		  <item>1985-90</item>
		  <item>1991</item>
		  <item>1992</item>
		</selectbox>
	  </element>
	  <element vname="Color" text ="Color Preference">
		<selectbox>
		  <item>White</item>
		  <item>Black</item>
		  <item>Blue</item>
		  <item>Red</item>
		</selectbox>
	  </element>
	  <element vname="Options" text ="items (check preferred)">	
		  <checkbox>
				<item> Air Conditioning	</item>
				<item> Power Windows</item>
				<item> 2 Door</item>
				<item> 4 Door</item>
				<item> Power Locks</item>
				<item> Cruise Control	</item>
				<item> Alloy Wheels		</item>
				<item> Anti-Lock Brakes	</item>
				<item> 1/2 Ton	</item>
				<item> One Ton	</item>
				<item> 3/4 Ton	</item>
				<item> Tow Package	</item>
				<item> Moon Roof	</item>
				<item> Sun Roof	</item>
				<item> Leather Interior	</item>
				<item> Cassette Player	</item>
				<item> Extended Cab	</item>
				<item> Crew Cab	</item>
				<item> Automatic	</item>
				<item> 5 Speed	</item>
				<item> CD Player	</item>
				<item> 4 Wheel Drive</item>
				<item> 2 Wheel Drive	</item>
				<item> Sliding-rear-window	</item>
				<item> Bench Seat</item>
				<item> Bucket Seats</item>
				<item> Long Bed	</item>
				<item> Short Bed</item>
			</checkbox>
	  </element>
	  <element vname="Pricetop" text ="Price Limit">
		<selectbox>
		  <item>No Limit</item>
		  <item>35,000-75,000</item>
		  <item>25,000-35,000</item>
		  <item>20,000-25,000</item>
		</selectbox>
	  </element>
	  <element vname="HowHeard" text ="How did you hear about us">
		<textbox></textbox>
	  </element>
	</element>
  </search>
</domain>
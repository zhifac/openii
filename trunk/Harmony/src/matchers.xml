<harmony>
  	<matchers>
	    <matcher id="org.mitre.harmony.matchers.voters.EditDistanceMatcher" default="true"/>
	    <matcher id="org.mitre.harmony.matchers.voters.DocumentationMatcher" default="true">
			<option id="UseName" type="checkbox" selected="true">Compare Names</option>
			<option id="UseDescription" type="checkbox" selected="true">Compare Descriptions</option>
	    </matcher>
	    <matcher id="org.mitre.harmony.matchers.voters.ExactStructureMatcher" default="true">
			<option id="UseName" type="checkbox" selected="true">Compare Names</option>
			<option id="UseDescription" type="checkbox" selected="true">Compare Descriptions</option>
	    </matcher>
	    <matcher id="org.mitre.harmony.matchers.voters.ThesaurusMatcher" default="true">
			<option id="UseName" type="checkbox" selected="true">Compare Names</option>
			<option id="UseDescription" type="checkbox" selected="true">Compare Descriptions</option>
	    </matcher>
	    <matcher id="org.mitre.harmony.matchers.voters.WordNetMatcher">
			<option id="UseName" type="checkbox" selected="true">Compare Names</option>
			<option id="UseDescription" type="checkbox" selected="true">Compare Descriptions</option>
	    </matcher>
	    <matcher id="org.mitre.harmony.matchers.voters.QuickDocumentationMatcher"/>
	    <matcher id="org.mitre.harmony.matchers.voters.DocumentationOnlyMatcher" hidden="true"/>
	    <matcher id="org.mitre.harmony.matchers.voters.ExactDocumentationMatcher" hidden="true"/>
	    <matcher id="org.mitre.harmony.matchers.voters.EntityMatcher" hidden="true"/>
  	</matchers>
  	<mergers>
	    <merger id="org.mitre.harmony.matchers.mergers.VoteMerger"/>
  	</mergers>
</harmony>
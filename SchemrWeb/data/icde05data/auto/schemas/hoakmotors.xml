<domain name="Automobile">  
	<search>
		<source name="HoakMotorsInc" url="http://www.hoakmotors.com"></source>
		<element>
			<element vname="d_vehicle_new_flag">
				<checkbox>
					<item>New</item>
					<item>Used</item>
				</checkbox>
		  	</element>	  
		  	<element vname="d_Make_required" text ="Vehicle Make">
				<selectbox>
			  		<item>Cadillac</item>
			  		<item>Chevrolet</item>
			  		<item>GMC</item>
			  		<item>Nissan</item>
			  		<item>Pontiac</item>
				</selectbox>
		  	</element>
		  	<element vname="d_model_name" text ="Vehicle Model" depends="d_Make_required">
				<selectbox>
			  		<item>Any Model</item>
			  		<item>Allante</item>
			  		<item>Brougham</item>
			  		<item>1500</item>
			  		<item>2500</item>
			  		<item>3500</item>
			  		<item>200SX</item>
			  		<item>210</item>
			  		<item>240SX</item>
			  		<item>6000</item>
				</selectbox>
		  	</element>
		  	<element vname= "d_minPrice" text ="Price Range">
			  	<selectbox>
					<item>All Prices</item>
					<item>$1,000</item>
					<item>$2,500</item>
					<item>$5,000</item>
					<item>$10,000></item>
					<item>$15,000</item>
					<item>$20,000</item>
					<item>$25,000</item>
					<item>$30,000</item>
					<item>$35,000</item>
			  </selectbox>
			</element>
			<element vname = "d_maxPrice" text="to">
				<selectbox>
					<item>All Prices</item>
					<item>$1,000</item>
					<item>$2,500</item>
					<item>$5,000</item>
					<item>$10,000></item>
					<item>$15,000</item>
					<item>$20,000</item>
					<item>$25,000</item>
					<item>$30,000</item>
					<item>$35,000</item>
			  	</selectbox>
			</element>
		  	<element vname="d_minYear" text ="Year Range 4-digit years, please">
				<textbox></textbox>
			</element>
			<element vname="d_maxYear"  text ="to">
			  	<textbox></textbox>
			</element>
		  	<element vname="d_Category" text ="Vehicle Body Style">
				<selectbox>
			  		<item>All Body Styles</item>
			  		<item>2Dr Coupe</item>
			  		<item>Convertible</item>
			  		<item>Hatchback</item>
			  		<item>4Dr Sedan</item>
			  		<item>Sport Utility</item>
			  		<item>Pickup Truck</item>
			  		<item>Van</item>
			  		<item>Wagon</item>
				</selectbox>
		  	</element>
		  	<element vname="d_maxMileage" text ="Vehicle Mileage">
				<selectbox>
			  		<item>Doesn't Matter</item>
			  		<item>Under 15,000</item>
			  		<item>Under 30,000</item>
			  		<item>Under 50,000</item>
			  		<item>Under 75,000</item>
				</selectbox>
		  	</element>
		  	<element vname="x_number_of_rows" text="Vehicles per page">
				<selectbox>
			  		<item>5</item>
			  		<item>10</item>
			  		<item>25</item>
			  		<item>50</item>
			  		<item>100</item>
			  		<item>200</item>
				</selectbox>
		  	</element>
		</element>
  	</search>
</domain>
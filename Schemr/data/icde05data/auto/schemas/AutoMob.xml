			<element vname = "EvePhone_3" >
				<textbox/>
			</element>
			<element vname = "EvePhone_Ext" text = "Ext">
				<textbox/>
			</element>
			<element vname="package" text = "Select Package">
				<selectbox>
		            <item>Type S Coupe</item>
		            <item>Coupe</item>
				<item>Red Label Sedan</item>
		            <item>T Sedan</item>

		        </selectbox>
			</element>
			<element vname="Exterior" text = "Select Color (Exterior)">
				<selectbox >
					<item>Aegean Blue</item>
					<item>Anthracite</item>
					<item>Eternal Blue</item>
					<item>Naples Gold</item>
					<item>Nighthawk Black</item>
					<item>Noble Green</item>
					<item>San Marino Red</item>
					<item>Satin Silver</item>
					<item>Taffeta White</item>
				</selectbox>
			</element>
			<element vname="Interior" text = "Select Color (Interior)">
				<selectbox>
		            <item>Ebony</item>
		            <item>Parchment</item>
		        </selectbox>
	        </element>
			<element vname="Best_Time_To_Contact" text = "Best Time To Contact">
				<selectbox>
		            <item>Anytime</item>
		            <item>Mornings</item>
		            <item>Afternoons</item>
		            <item>Evenings</item>
		            <item>Business Hours</item>
		            <item>Weekends</item>
	            </selectbox>
	        </element>
	        <element vname="time_frame" text = "Your timeframe">
	        	<selectbox>
		            <item>Within 48 hours</item>
		            <item>Within a week</item>
		            <item>Within two weeks</item>
		            <item>Within a month</item>
		        </selectbox>
		    </element>
			<element vname="finance" text = "Payment Method">
		        <selectbox>
		            <item>Lease</item>
		            <item>Loan</item>
		            <item>Cash</item>
		            <item>Credit Union</item>
		        </selectbox>
	        </element>
	        <element vname = "Notes_to_dealer" text = "Additional Comments (optional)">
	        	<textbox/>
	        </element>		    
		</element>   
	</search>        
</domain>            
                     
                     